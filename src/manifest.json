{
    "manifest_version": 3,
    "name": "Enhanced Enroll",
    "version": "0.1.0",
    "description": "Enhancing your Course, Search, & Enroll expierence",
    "action": {
        "default_popup": "popup.html",
        "default_title": "Enhanced Enroll"
    },
    "permissions": [
        "storage",
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://enroll.wisc.edu/search"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "background": {
        "service_worker": "background.js"
    }
}