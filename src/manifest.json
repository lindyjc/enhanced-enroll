{
    "manifest_version": 3,
    "name": "Enhanced Enroll",
    "version": "0.1.0",
    "description": "Enhancing your Course, Search, & Enroll expierence",
    "action": {
        "default_popup": "popup.html",
        "default_title": "Enhanced Enroll"
    },
    "permissions": [
        "storage",
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://enroll.wisc.edu/*"
            ],
            "js": [
                "content.js"
            ],
            "type": "module"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "injected.css",
                "node_modules/plotly.js-dist/plotly.js",
                "madgrades/config.js",
                "madgrades/madgrades_handler.js"
            ],
            "matches": [
                "https://enroll.wisc.edu/*"
            ]
        }
    ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    }
}